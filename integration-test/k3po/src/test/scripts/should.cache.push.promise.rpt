     [0x00]                                                 # DATA frame
     [0x01]                                                 # END_STREAM
     [0x00 0x00 0x00 0x01]                                  # stream_id = 1

write [0x00 0x00 0x57]                                      # length = 87
      [0x01]                                                # HEADERS frame
      [0x04]                                                # END_HEADERS
      [0x00 0x00 0x00 0x01]                                 # stream_id = 1
      [0x88]                                                # :status: 200
      [0x0f 0x27] [0x14] "CERN/3.0 libwww/2.17"             # server
