      [0x00 0x00 0x00 0x00]                                 # stream_id = 0

read [0x00 0x00 0x0b]                                       # length = 11
     [0x01]                                                 # HEADERS frame
     [0x04]                                                 # END_HEADERS
     [0x00 0x00 0x00 0x01]                                  # stream_id = 1
