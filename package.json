{
  "name": "http2-cache",
  "version": "1.0.0",
  "description": "Exposes http caching to the browser by adding functionality to XMLHttpRequest, and then running XMLHttpRequest over http2 over WebSockets",
  "main": "index.js",
  "scripts": {
    "prestartk3po": "echo 'pre startK3po'",
    "startk3po": "echo 'startK3po'",
    "pretest": "browserify lib/http2-ws.js -o builds/bundle.js && nohup java -jar java-artifacts-TODO-move/ry-develop-SNAPSHOT.jar -script test/reaktivity/ws-echo.js > /dev/null 2>&1 & echo $! > run.pid",
    "test": "mocha",
    "preintegration-test": "node ./integration-test/reaktivity/reactivity.js start && node ./integration-test/k3po/k3po.js start",
    "integration-test": "mocha integration-test/*.js",
    "postintegration-test": "node ./integration-test/reaktivity/reactivity.js stop && node ./integration-test/k3po/k3po.js stop",
    "build": "npm run test && npm run integration-test",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/montagejs/http2-cache.git"
  },
  "keywords": [
    "http2",
    "js",
    "sandbox",
    "push"
  ],
  "author": "dpwspoon",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/montagestudio/http2-cache/issues"
  },
  "homepage": "https://github.com/montagestudio/http2-cache#readme",
  "dependencies": {
    "spdy-transport": "2.0.18"
  },
  "devDependencies": {
    "websocket": "1.0.24 ",
    "browserify": "14.0.0 ",
    "xhr2": "0.1.4",
    "spdy": "3.4.4 ",
    "sinon": "1.17.7",
    "mocha": "3.2.0"
  }
}
